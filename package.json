{
  "name": "fullstack-react-node",
  "version": "0.0.1",
  "description": "A Boilerplate for building fullstack applications with React, React Native and Node.",
  "author": "Brijesh Bhakta <brijeshbhakta30@gmail.com>",
  "private": false,
  "scripts": {
    "android": "react-native run-android",
    "build": "craco build",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start:native": "react-native start",
    "test": "yarn test:native",
    "test:native": "jest",
    "web": "craco start"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brijeshbhakta30/fullstack-react-node.git"
  },
  "dependencies": {
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-dom": "16.11.0",
    "react-native": "0.61.4",
    "react-scripts": "3.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@babel/runtime": "7.7.2",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@craco/craco": "5.6.2",
    "@react-native-community/eslint-config": "0.0.5",
    "babel-jest": "24.9.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.5.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.7.0",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "lint-staged": "9.4.2",
    "metro-react-native-babel-preset": "0.57.0",
    "prettier": "1.19.0",
    "react-test-renderer": "16.9.0"
  },
  "keywords": [
    "fullstack",
    "react",
    "react-native",
    "node",
    "node.js",
    "starter",
    "starter code",
    "es6",
    "es7",
    "boilerplate"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "preset": "react-native",
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && yarn test"
    }
  }
}